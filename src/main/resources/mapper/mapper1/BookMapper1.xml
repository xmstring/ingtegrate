<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multi.datasource.integrate.mapper.mapper1.BookMapper1">

    <select id="getAllBooks1" resultType="com.multi.datasource.integrate.pojo.Book">
        select * from book
    </select>
    <insert id="insertBook1" parameterType="com.multi.datasource.integrate.pojo.Book">
       insert into book(id,name,author) values (#{id},#{name},#{author})
   </insert>
</mapper>
